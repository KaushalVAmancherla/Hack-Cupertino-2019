package com.example.hackathonreredo;


import java.lang.String;
import android.support.v7.app.AppCompatActivity;
import android.os.Bundle;
import android.view.View;
import android.widget.TextView;

public class MainActivity extends AppCompatActivity
{

    String number = "";
    String d1 = "Good Job. Calling 911 does not require service";
    String d2 = "Sorry, you have no service. What number can you call?";
    @Override
    protected void onCreate(Bundle savedInstanceState) {
        super.onCreate(savedInstanceState);
        setContentView(R.layout.activity_main);
    }



    public void onClick(View view)
    {
        number = number + 1;
        TextView textview = (TextView)findViewById(R.id.textView);
        textview.setText(number);
    }

    public void butt2(View view)
    {
        number = number + 2;
        TextView textview = (TextView)findViewById(R.id.textView);
        textview.setText(number);
    }

    public void butt3(View view)
    {
        number = number + 3;
        TextView textview = (TextView)findViewById(R.id.textView);
        textview.setText(number);
    }

    public void butt4(View view)
    {
        number = number + 4;
        TextView textview = (TextView)findViewById(R.id.textView);
        textview.setText(number);
    }

    public void butt5(View view)
    {
        number = number + 5;
        TextView textview = (TextView)findViewById(R.id.textView);
        textview.setText(number);
    }

    public void butt6(View view)
    {
        number = number + 6;
        TextView textview = (TextView)findViewById(R.id.textView);
        textview.setText(number);
    }

    public void butt7(View view)
    {
        number = number + 7;
        TextView textview = (TextView)findViewById(R.id.textView);
        textview.setText(number);
    }

    public void butt8(View view)
    {
        number = number + 8;
        TextView textview = (TextView)findViewById(R.id.textView);
        textview.setText(number);
    }

    public void butt9(View view)
    {
        number = number + 9;
        TextView textview = (TextView)findViewById(R.id.textView);
        textview.setText(number);
    }

    public void butt0(View view)
    {
        number = number + 0;
        TextView textview = (TextView)findViewById(R.id.textView);
        textview.setText(number);
    }

    public void isCalled(View view)
    {
        TextView textView2 = (TextView) findViewById(R.id.textView2);
        if (number.equalsIgnoreCase("911"))
        {
            textView2.setText(d1);
        }
        else
        {
            textView2.setText(d2);
        }
    }

    public void delete()
    {
        int value = Integer.parseInt(number)/10;
        TextView textview = (TextView)findViewById(R.id.textView);
        textview.setText(value);
    }
}
